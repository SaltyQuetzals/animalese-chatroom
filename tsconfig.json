{
  "compilerOptions": {
    "lib": [
      "ES5",
      "ES6",
      "DOM"
    ],
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./build",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "strictNullChecks": true
  }
}